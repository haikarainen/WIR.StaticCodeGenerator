<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <VulkanSDK>$(VULKAN_SDK)</VulkanSDK>
    <WIRFramework>$(ProjectDir)../WIR.Framework/</WIRFramework>
    <LLVM>C:/Program Files/LLVM/</LLVM>
    <WIROutputDir>$(ProjectDir)\..\WIR.Output\</WIROutputDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="VulkanSDK">
      <Value>$(VulkanSDK)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="WIRFramework">
      <Value>$(WIRFramework)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="LLVM">
      <Value>$(LLVM)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="WIROutputDir">
      <Value>$(WIROutputDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>